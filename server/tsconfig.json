{
  "include": ["**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "lib": ["es2023", "dom"],
    "module": "NodeNext",
    "target": "es2022",
    "outDir": "./dist/",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext"
  }
}
